{
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "blink",
    "workbench.colorTheme": "Nord",
    "editor.hideCursorInOverviewRuler": true,
    "editor.highlightActiveIndentGuide": false,
    "editor.renderLineHighlight": "none",
    "terminal.integrated.enableBell": true,
    "editor.fontSize": 17,
    "editor.minimap.enabled": false,
    "editor.rulers": [80,100],
    "editor.renderIndentGuides": false,
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "onFocusChange",
    "editor.fontFamily": "Office Code Pro",
    "breadcrumbs.enabled": true,
    "html.format.wrapLineLength": 0,
    "diffEditor.renderSideBySide": false,
    "gitlens.currentLine.scrollable": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.historyExplorer.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.activeIconPack": "none",
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.files.associations": {
        ".env.staging": "settings",
        ".env.production": "settings",
        ".env.development": "settings",
        ".env": "settings",
        "*.go": "Go_gopher"
    },
    "editor.occurrencesHighlight": false,
    "window.newWindowDimensions": "maximized",
    "files.useExperimentalFileWatcher": true,
    "trailing-spaces.syntaxIgnore": [
        "markdown"
    ],
    "editor.links": false,
    "workbench.startupEditor": "welcomePage",
    "editor.matchBrackets": "never",
    "ruby.intellisense": "rubyLocate",
    "[shellscript]": {
        "editor.tabSize": 2
    },
    "[go]": {
        "editor.tabSize": 2
    },
    "window.restoreWindows": "none",
    "json.format.enable": false,
    "html.format.enable": false,
    "window.zoomLevel": 0,
    "window.nativeTabs": true,
    "window.title": "${rootPath}${separator}${activeEditorShort}",
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "workbench.settings.editor": "json",
    "terminal.integrated.fontSize": 17,
    "bracketPairColorizer.scopeLineRelativePosition": false,
    "bracketPairColorizer.showHorizontalScopeLine": false,
    "bracketPairColorizer.showVerticalScopeLine": false,
    "bracketPairColorizer.colorMode": "Consecutive",
    "jest.coverageFormatter": "GutterFormatter",
    "jest.enableCodeLens": false,
    "jest.enableSnapshotPreviews": false,
    "jest.enableSnapshotUpdateMessages": false,
    "jest.runAllTestsFirst": false,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-n>"],
            "commands": [":nohl"]
        },
        {
            "before": ["<leader>", "w"],
            "commands": ["workbench.action.files.save"]
        }
    ]
}
